     SUM(CASE
          WHEN acc_info.conversion_date IS NULL -- This account uses transaction rate
           THEN (via_currency_converter(COALESCE(ji.debit, 0.0) - COALESCE(ji.credit, 0.0),
                                        acc_info.rates_chain,
                                        '$P!{AS_OF_DATE_2_YR}-$P!{AS_OF_DATE_2_MO}-$P!{AS_OF_DATE_2_DY}'::DATE)
                 - (ji.top_level_debit - ji.top_level_credit))
          ELSE 0.0
         END) AS unrealized_gain_or_loss,
     NULL AS move_lines_count,
     NULL AS move_lines
    FROM journal_items_cmp_1 ji
     INNER JOIN (SELECT DISTINCT id, rates_chain, conversion_date
                 FROM coa_tree_currency) acc_info
      ON acc_info.id = ji.account_id
    WHERE date::DATE
     BETWEEN '$P!{FISCALYEAR_START_YR_2}-$P!{FISCALYEAR_START_MO_2}-$P!{FISCALYEAR_START_DY_2}'
      AND ('$P!{FROM_DATE_2_YR_2}-$P!{FROM_DATE_2_MO_2}-$P!{FROM_DATE_2_DY_2}'::DATE - '1 day'::INTERVAL)::DATE
    GROUP BY
     ji.account_id,
     ji.company_id,
     acc_info.rates_chain,
     acc_info.conversion_date
   )
  SELECT
   COALESCE(bb.acc_id, mv.acc_id) AS acc_id,
   2 AS comparison_id,
   COALESCE(bb.com_id, mv.com_id) AS com_id,
   COALESCE(bb.top_level_debit, 0.0) AS bb_dr,
   COALESCE(bb.top_level_credit, 0.0) AS bb_cr,
   COALESCE(bb.unrealized_gain_or_loss, 0.0) AS bb_unrealized_gain_or_loss,
   COALESCE(mv.top_level_debit, 0.0) AS dr,
   COALESCE(mv.top_level_credit, 0.0) AS cr,
   COALESCE(mv.unrealized_gain_or_loss, 0.0) AS unrealized_gain_or_loss,
   COALESCE(mv.move_lines_count, 0) AS move_lines_count,
   COALESCE(mv.move_lines, ARRAY[]::BIGINT[]) AS move_lines
  FROM movement mv
   FULL JOIN beginning_balance bb
    ON mv.acc_id = bb.acc_id
 ),
 axes_data_points AS (
  SELECT
   axes.acc_id,
   axes.acc_parent_id,
   axes.acc_level,
   axes.acc_parent_left,
   axes.acc_parent_right,
   axes.comparison_id,
   axes.com_order,
   axes.com_id,
   -- Reference data
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.bb_dr, 0.0)
     ELSE data_points.bb_dr
    END) AS bb_dr,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.bb_cr, 0.0)
     ELSE data_points.bb_cr
    END) AS bb_cr,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.bb_unrealized_gain_or_loss, 0.0)
     ELSE data_points.bb_unrealized_gain_or_loss
    END) AS bb_unrealized_gain_or_loss,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.dr, 0.0)
     ELSE data_points.dr
    END) AS dr,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.cr, 0.0)
     ELSE data_points.cr
    END) AS cr,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.unrealized_gain_or_loss, 0.0)
     ELSE data_points.unrealized_gain_or_loss
    END) AS unrealized_gain_or_loss,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.move_lines_count, 0.0)
     ELSE data_points.move_lines_count
    END) AS move_lines_count,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points.move_lines, ARRAY[]::BIGINT[])
     ELSE data_points.move_lines
    END) AS move_lines,
   -- Comparison 1
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.bb_dr, 0.0)
     ELSE data_points_cmp_1.bb_dr
    END) AS bb_dr_cmp_1,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.bb_cr, 0.0)
     ELSE data_points_cmp_1.bb_cr
    END) AS bb_cr_cmp_1,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.bb_unrealized_gain_or_loss, 0.0)
     ELSE data_points_cmp_1.bb_unrealized_gain_or_loss
    END) AS bb_unrealized_gain_or_loss_cmp_1,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.dr, 0.0)
     ELSE data_points_cmp_1.dr
    END) AS dr_cmp_1,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.cr, 0.0)
     ELSE data_points_cmp_1.cr
    END) AS cr_cmp_1,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.unrealized_gain_or_loss, 0.0)
     ELSE data_points_cmp_1.unrealized_gain_or_loss
    END) AS unrealized_gain_or_loss_cmp_1,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.move_lines_count, 0.0)
     ELSE data_points_cmp_1.move_lines_count
    END) AS move_lines_count_cmp_1,
   (CASE
     WHEN aa.company_id = axes.com_id
      THEN COALESCE(data_points_cmp_1.move_lines, ARRAY[]::BIGINT[])
     ELSE data_points_cmp_1.move_lines
    END) AS move_lines_cmp_1
  FROM axes
   INNER JOIN account_account aa
    ON aa.id = axes.acc_id
   LEFT JOIN data_points
    ON (axes.acc_id,
        axes.comparison_id,
        axes.com_id) = (data_points.acc_id,
                        data_points.comparison_id,
                        data_points.com_id)
   LEFT JOIN data_points_cmp_1
    ON (axes.acc_id,
        axes.comparison_id,
        axes.com_id) = (data_points_cmp_1.acc_id,
                        data_points_cmp_1.comparison_id,
                        data_points_cmp_1.com_id)
 ),
 coa_tree_calculated_sans_gain_loss(acc_id, acc_parent_id, acc_level, acc_parent_left, acc_parent_right,
                                    comparison_id,
                                    com_order, com_id,
                                    bb_dr, bb_cr, bb_unrealized_gain_or_loss, dr, cr, unrealized_gain_or_loss, move_lines_count, move_lines,
                                    bb_dr_cmp_1, bb_cr_cmp_1, bb_unrealized_gain_or_loss_cmp_1, dr_cmp_1, cr_cmp_1, unrealized_gain_or_loss_cmp_1, move_lines_count_cmp_1, move_lines_cmp_1) AS (
  (SELECT
    adp.acc_id,
    adp.acc_parent_id,
    adp.acc_level,
    adp.acc_parent_left,
    adp.acc_parent_right,
    adp.comparison_id,
    adp.com_order,
    adp.com_id,
    adp.bb_dr,
    adp.bb_cr,
    adp.bb_unrealized_gain_or_loss,
    adp.dr,
    adp.cr,
    adp.unrealized_gain_or_loss,
    adp.move_lines_count::NUMERIC,
    adp.move_lines::BIGINT[],
    adp.bb_dr_cmp_1,
    adp.bb_cr_cmp_1,
    adp.bb_unrealized_gain_or_loss_cmp_1,
    adp.dr_cmp_1,
    adp.cr_cmp_1,
    adp.unrealized_gain_or_loss_cmp_1,
    adp.move_lines_count_cmp_1::NUMERIC,
    adp.move_lines_cmp_1::BIGINT[]
   FROM axes_data_points adp
   WHERE adp.acc_level = (SELECT MAX(level)
                          FROM coa_tree_currency)
  ) UNION ALL (
   WITH
    frozen_coa_tree_calculated AS (
     SELECT * FROM coa_tree_calculated_sans_gain_loss
    ),
    this_level_state AS (
     SELECT
      adp.acc_id,
      adp.acc_parent_id,
      adp.acc_level,
      adp.acc_parent_left,
      adp.acc_parent_right,
      adp.comparison_id,
      adp.com_order,
      adp.com_id,
      COALESCE(SUM(fctc.bb_dr) + COALESCE(adp.bb_dr, 0.0), adp.bb_dr) AS bb_dr,
      COALESCE(SUM(fctc.bb_cr) + COALESCE(adp.bb_cr, 0.0), adp.bb_cr) AS bb_cr,
      COALESCE(SUM(fctc.bb_unrealized_gain_or_loss) + COALESCE(adp.bb_unrealized_gain_or_loss, 0.0), adp.bb_unrealized_gain_or_loss) AS bb_unrealized_gain_or_loss,
      COALESCE(SUM(fctc.dr) + COALESCE(adp.dr, 0.0), adp.dr) AS dr,
      COALESCE(SUM(fctc.cr) + COALESCE(adp.cr, 0.0), adp.cr) AS cr,
      COALESCE(SUM(fctc.unrealized_gain_or_loss) + COALESCE(adp.unrealized_gain_or_loss, 0.0), adp.unrealized_gain_or_loss) AS unrealized_gain_or_loss,
      (CASE aa.type
        WHEN 'consolidation'
         THEN COALESCE(SUM(fctc.move_lines_count) + COALESCE(adp.move_lines_count, 0.0), adp.move_lines_count)
        ELSE adp.move_lines_count
       END) AS move_lines_count,
      (CASE aa.type
        WHEN 'consolidation'
         THEN COALESCE(GL_ARRAY_REDUCE(fctc.move_lines) || COALESCE(adp.move_lines, ARRAY[]::BIGINT[]), adp.move_lines)
        ELSE
         adp.move_lines
       END) AS move_lines,
      COALESCE(SUM(fctc.bb_dr_cmp_1) + COALESCE(adp.bb_dr_cmp_1, 0.0), adp.bb_dr_cmp_1) AS bb_dr_cmp_1,
      COALESCE(SUM(fctc.bb_cr_cmp_1) + COALESCE(adp.bb_cr_cmp_1, 0.0), adp.bb_cr_cmp_1) AS bb_cr_cmp_1,
      COALESCE(SUM(fctc.bb_unrealized_gain_or_loss_cmp_1) + COALESCE(adp.bb_unrealized_gain_or_loss_cmp_1, 0.0), adp.bb_unrealized_gain_or_loss_cmp_1) AS bb_unrealized_gain_or_loss_cmp_1,
      COALESCE(SUM(fctc.dr_cmp_1) + COALESCE(adp.dr_cmp_1, 0.0), adp.dr_cmp_1) AS dr_cmp_1,
      COALESCE(SUM(fctc.cr_cmp_1) + COALESCE(adp.cr_cmp_1, 0.0), adp.cr_cmp_1) AS cr_cmp_1,
      COALESCE(SUM(fctc.unrealized_gain_or_loss_cmp_1) + COALESCE(adp.unrealized_gain_or_loss_cmp_1, 0.0), adp.unrealized_gain_or_loss_cmp_1) AS unrealized_gain_or_loss_cmp_1,
      (CASE aa.type
        WHEN 'consolidation'
         THEN COALESCE(SUM(fctc.move_lines_count_cmp_1) + COALESCE(adp.move_lines_count_cmp_1, 0.0), adp.move_lines_count_cmp_1)
        ELSE adp.move_lines_count_cmp_1
       END) AS move_lines_count_cmp_1,
      (CASE aa.type
        WHEN 'consolidation'
         THEN COALESCE(GL_ARRAY_REDUCE(fctc.move_lines_cmp_1) || COALESCE(adp.move_lines_cmp_1, ARRAY[]::BIGINT[]), adp.move_lines_cmp_1)
        ELSE adp.move_lines_cmp_1
       END) AS move_lines_cmp_1
     FROM axes_data_points adp
      INNER JOIN account_account aa
       ON adp.acc_id = aa.id
      LEFT JOIN frozen_coa_tree_calculated fctc
       ON (adp.acc_level = fctc.acc_level - 1
           AND adp.acc_parent_left < fctc.acc_parent_left
           AND adp.acc_parent_right > fctc.acc_parent_right
           AND adp.comparison_id = fctc.comparison_id
           AND adp.com_id = fctc.com_id)
     WHERE
      adp.acc_level = (SELECT DISTINCT acc_level - 1
                       FROM frozen_coa_tree_calculated)
     GROUP BY
      aa.type,
      adp.acc_id,
      adp.acc_parent_id,
      adp.acc_level,
      adp.acc_parent_left,
      adp.acc_parent_right,
      adp.comparison_id,
      adp.com_order,
      adp.com_id,
      adp.bb_dr,
      adp.bb_cr,
      adp.bb_unrealized_gain_or_loss,
      adp.dr,
      adp.cr,
      adp.unrealized_gain_or_loss,
      adp.move_lines_count,
      adp.move_lines,
      adp.bb_dr_cmp_1,
      adp.bb_cr_cmp_1,
      adp.bb_unrealized_gain_or_loss_cmp_1,
      adp.dr_cmp_1,
      adp.cr_cmp_1,
      adp.unrealized_gain_or_loss_cmp_1,
      adp.move_lines_count_cmp_1,
      adp.move_lines_cmp_1
    )
   SELECT
    acc_id,
    acc_parent_id,
    acc_level,
    acc_parent_left,
    acc_parent_right,
    comparison_id,
    com_order,
    com_id,
    SUM(bb_dr) AS bb_dr,
    SUM(bb_cr) AS bb_cr,
    SUM(bb_unrealized_gain_or_loss) AS bb_unrealized_gain_or_loss,
    SUM(dr) AS dr,
    SUM(cr) AS cr,
    SUM(unrealized_gain_or_loss) AS unrealized_gain_or_loss,
    SUM(move_lines_count) AS move_lines_count,
    GL_ARRAY_REDUCE(move_lines) AS move_lines,
    SUM(bb_dr_cmp_1) AS bb_dr_cmp_1,
    SUM(bb_cr_cmp_1) AS bb_cr_cmp_1,
    SUM(bb_unrealized_gain_or_loss_cmp_1) AS bb_unrealized_gain_or_loss_cmp_1,
    SUM(dr_cmp_1) AS dr_cmp_1,
    SUM(cr_cmp_1) AS cr_cmp_1,
    SUM(unrealized_gain_or_loss_cmp_1) AS unrealized_gain_or_loss_cmp_1,
    SUM(move_lines_count_cmp_1) AS move_lines_count_cmp_1,
    GL_ARRAY_REDUCE(move_lines_cmp_1) AS move_lines_cmp_1
   FROM
    ((SELECT
       tls.acc_id,
       tls.acc_parent_id,
       tls.acc_level,
       tls.acc_parent_left,
       tls.acc_parent_right,
       tls.comparison_id,
       tls.com_order,
       tls.com_id,
       tls.bb_dr,
       tls.bb_cr,
       tls.bb_unrealized_gain_or_loss,
       tls.dr,
       tls.cr,
       tls.unrealized_gain_or_loss,
       tls.move_lines_count,
       tls.move_lines,
       tls.bb_dr_cmp_1,
       tls.bb_cr_cmp_1,
       tls.bb_unrealized_gain_or_loss_cmp_1,
       tls.dr_cmp_1,
       tls.cr_cmp_1,
       tls.unrealized_gain_or_loss_cmp_1,
       tls.move_lines_count_cmp_1,
       tls.move_lines_cmp_1
      FROM this_level_state tls
     )
     UNION ALL
     (SELECT
       tls.acc_id,
       tls.acc_parent_id,
       tls.acc_level,
       tls.acc_parent_left,
       tls.acc_parent_right,
       tls.comparison_id,
       tls.com_order,
       tls.com_id,
       SUM(consol_child.bb_dr) AS bb_dr,
       SUM(consol_child.bb_cr) AS bb_cr,
       SUM(consol_child.bb_unrealized_gain_or_loss) AS bb_unrealized_gain_or_loss,
       SUM(consol_child.dr) AS dr,
       SUM(consol_child.cr) AS cr,
       SUM(consol_child.unrealized_gain_or_loss) AS unrealized_gain_or_loss,
       SUM(consol_child.move_lines_count) AS move_lines_count,
       GL_ARRAY_REDUCE(consol_child.move_lines) AS move_lines,
       SUM(consol_child.bb_dr_cmp_1) AS bb_dr_cmp_1,
       SUM(consol_child.bb_cr_cmp_1) AS bb_cr_cmp_1,
       SUM(consol_child.bb_unrealized_gain_or_loss_cmp_1) AS bb_unrealized_gain_or_loss_cmp_1,
       SUM(consol_child.dr_cmp_1) AS dr_cmp_1,
       SUM(consol_child.cr_cmp_1) AS cr_cmp_1,
       SUM(consol_child.unrealized_gain_or_loss_cmp_1) AS unrealized_gain_or_loss_cmp_1,
       SUM(consol_child.move_lines_count_cmp_1) AS move_lines_count_cmp_1,
       GL_ARRAY_REDUCE(consol_child.move_lines_cmp_1) AS move_lines_cmp_1
      FROM this_level_state tls
       INNER JOIN account_account aa
        ON (tls.acc_id,
            tls.com_id) = (aa.id,
                           aa.company_id)
       INNER JOIN company_tree ct
        ON tls.com_id = ct.id
       INNER JOIN this_level_state consol_child
        ON (consol_child.acc_parent_left,
            consol_child.comparison_id) = (tls.acc_parent_left,
                                           tls.comparison_id)
       INNER JOIN company_tree consol_ct
        ON consol_child.com_id = consol_ct.id
      WHERE
       aa.type = 'consolidation'
       AND consol_child.com_id != tls.com_id
       AND ct.level = consol_ct.level - 1
      GROUP BY
       tls.acc_id,
       tls.acc_parent_id,
       tls.acc_level,
       tls.acc_parent_left,
       tls.acc_parent_right,
       tls.comparison_id,
       tls.com_order,
       tls.com_id
     )
    ) consolidation_calculated
   GROUP BY
    acc_id,
    acc_parent_id,
    acc_parent_left,
    acc_parent_right,
    acc_level,
    comparison_id,
    com_order,
    com_id
  )
 ),
 currency_gain_or_loss_acc_info AS (
  WITH currency_gain_or_loss_total AS (
   SELECT
    bb_unrealized_gain_or_loss,
    unrealized_gain_or_loss,
    bb_unrealized_gain_or_loss_cmp_1,
    unrealized_gain_or_loss_cmp_1
   FROM coa_tree_calculated_sans_gain_loss
   WHERE
    acc_parent_id IS NULL
    AND com_id = $P!{COMPANY_ID}
  )
  SELECT
   ctcsgl.acc_id,
   ctcsgl.acc_parent_left,
   ctcsgl.acc_parent_right,
   (CASE
     WHEN ctcsgl_datum.bb_unrealized_gain_or_loss >= 0.0
      THEN ctcsgl_datum.bb_unrealized_gain_or_loss
     ELSE 0.0
    END) AS bb_unrealized_gain_or_loss_dr,
   (CASE
     WHEN ctcsgl_datum.bb_unrealized_gain_or_loss < 0.0
      THEN -ctcsgl_datum.bb_unrealized_gain_or_loss
     ELSE 0.0
    END) AS bb_unrealized_gain_or_loss_cr,
   (CASE
     WHEN ctcsgl_datum.unrealized_gain_or_loss >= 0.0
      THEN ctcsgl_datum.unrealized_gain_or_loss
     ELSE 0.0
    END) AS unrealized_gain_or_loss_dr,
   (CASE
     WHEN ctcsgl_datum.unrealized_gain_or_loss < 0.0
      THEN -ctcsgl_datum.unrealized_gain_or_loss
     ELSE 0.0
    END) AS unrealized_gain_or_loss_cr,
   (CASE
     WHEN ctcsgl_datum.bb_unrealized_gain_or_loss_cmp_1 >= 0.0
      THEN ctcsgl_datum.bb_unrealized_gain_or_loss_cmp_1
     ELSE 0.0
    END) AS bb_unrealized_gain_or_loss_dr_cmp_1,
   (CASE
     WHEN ctcsgl_datum.bb_unrealized_gain_or_loss_cmp_1 < 0.0
      THEN -ctcsgl_datum.bb_unrealized_gain_or_loss_cmp_1
     ELSE 0.0
    END) AS bb_unrealized_gain_or_loss_cr_cmp_1,
   (CASE
     WHEN ctcsgl_datum.unrealized_gain_or_loss_cmp_1 >= 0.0
      THEN ctcsgl_datum.unrealized_gain_or_loss_cmp_1
     ELSE 0.0
    END) AS unrealized_gain_or_loss_dr_cmp_1,
   (CASE
     WHEN ctcsgl_datum.unrealized_gain_or_loss_cmp_1 < 0.0
      THEN -ctcsgl_datum.unrealized_gain_or_loss_cmp_1
     ELSE 0.0
    END) AS unrealized_gain_or_loss_cr_cmp_1
  FROM coa_tree_calculated_sans_gain_loss ctcsgl
   INNER JOIN currency_gain_or_loss_total ctcsgl_datum
    ON TRUE
  WHERE
   ctcsgl.acc_id = $P!{GAIN_OR_LOSS_ACC_ID}
   AND ctcsgl.com_id = $P!{COMPANY_ID}
 ),
 coa_tree_calculated AS (
  SELECT
   ctcsgl.acc_id,
   ctcsgl.acc_parent_id,
   ctcsgl.acc_level,
   ctcsgl.acc_parent_left,
   ctcsgl.acc_parent_right,
   ctcsgl.comparison_id,
   ctcsgl.com_order,
   ctcsgl.com_id,
   ctcsgl.bb_dr + COALESCE(SUM(bb_unrealized_gain_or_loss_dr), 0.0) AS bb_dr,
   ctcsgl.bb_cr + COALESCE(SUM(bb_unrealized_gain_or_loss_cr), 0.0) AS bb_cr,
   ctcsgl.dr + COALESCE(SUM(unrealized_gain_or_loss_dr), 0.0) AS dr,
   ctcsgl.cr + COALESCE(SUM(unrealized_gain_or_loss_cr), 0.0) AS cr,
   ctcsgl.move_lines_count,
   ctcsgl.move_lines,
   ctcsgl.bb_dr_cmp_1 + COALESCE(SUM(bb_unrealized_gain_or_loss_dr_cmp_1), 0.0) AS bb_dr_cmp_1,
   ctcsgl.bb_cr_cmp_1 + COALESCE(SUM(bb_unrealized_gain_or_loss_cr_cmp_1), 0.0) AS bb_cr_cmp_1,
   ctcsgl.dr_cmp_1 + COALESCE(SUM(unrealized_gain_or_loss_dr_cmp_1), 0.0) AS dr_cmp_1,
   ctcsgl.cr_cmp_1 + COALESCE(SUM(unrealized_gain_or_loss_cr_cmp_1), 0.0) AS cr_cmp_1,
   ctcsgl.move_lines_count_cmp_1,
   ctcsgl.move_lines_cmp_1
  FROM coa_tree_calculated_sans_gain_loss ctcsgl
   LEFT JOIN currency_gain_or_loss_acc_info cgolai
    ON (ctcsgl.acc_parent_left <= cgolai.acc_parent_left
        AND ctcsgl.acc_parent_right >= cgolai.acc_parent_right)
  GROUP BY
   ctcsgl.acc_id,
   ctcsgl.acc_parent_id,
   ctcsgl.acc_level,
   ctcsgl.acc_parent_left,
   ctcsgl.acc_parent_right,
   ctcsgl.comparison_id,
   ctcsgl.com_order,
   ctcsgl.com_id,
   ctcsgl.bb_dr,
   ctcsgl.bb_cr,
   ctcsgl.dr,
   ctcsgl.cr,
   ctcsgl.move_lines_count,
   ctcsgl.move_lines,
   ctcsgl.bb_dr_cmp_1,
   ctcsgl.bb_cr_cmp_1,
   ctcsgl.dr_cmp_1,
   ctcsgl.cr_cmp_1,
   ctcsgl.move_lines_count_cmp_1,
   ctcsgl.move_lines_cmp_1
 ),
 reporting_tree_axes AS (
  SELECT
   -- First axis
   rtn.id AS rtn_id,
   rtn.parent_id AS rtn_parent_id,
   rtn.level AS rtn_level,
   rtn.parent_left AS rtn_parent_left,
   rtn.parent_right AS rtn_parent_right,
   -- Second axis
   comparison_count AS comparison_id,
   -- Third axis
   company_tree.parent_left AS com_order,
   company_tree.id AS com_id
  FROM via_reporting_tree_node rtn,
   company_tree,
   GENERATE_SERIES(1,
                   $P!{DISPLAY_COMPARISON}::INTEGER + 1,
                   1) comparison_count
  WHERE
   rtn.tree_id = $P!{REPORTING_TREE_ID}
 ),
 reporting_tree_data_points AS (
  SELECT
   rtn.id AS rtn_id,
   calculated_tree.comparison_id,
   calculated_tree.com_id,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.bb_dr * atn.multiplier / curr.rounding) * curr.rounding
       END) AS bb_dr,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.bb_cr * atn.multiplier / curr.rounding) * curr.rounding
       END) AS bb_cr,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.dr * atn.multiplier / curr.rounding) * curr.rounding
       END) AS dr,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.cr * atn.multiplier / curr.rounding) * curr.rounding
       END) AS cr,
   SUM(calculated_tree.move_lines_count) AS move_lines_count,
   GL_ARRAY_REDUCE(calculated_tree.move_lines) AS move_lines,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.bb_dr_cmp_1 * atn.multiplier / curr.rounding) * curr.rounding
       END) AS bb_dr_cmp_1,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.bb_cr_cmp_1 * atn.multiplier / curr.rounding) * curr.rounding
       END) AS bb_cr_cmp_1,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.dr_cmp_1 * atn.multiplier / curr.rounding) * curr.rounding
       END) AS dr_cmp_1,
   SUM(CASE
        WHEN curr.rounding = 0 THEN 0.0
        ELSE ROUND(calculated_tree.cr_cmp_1 * atn.multiplier / curr.rounding) * curr.rounding
       END) AS cr_cmp_1,
   SUM(calculated_tree.move_lines_count_cmp_1) AS move_lines_count_cmp_1,
   GL_ARRAY_REDUCE(calculated_tree.move_lines_cmp_1) AS move_lines_cmp_1
  FROM via_reporting_tree_node rtn
   INNER JOIN via_account_tree_node atn
    ON rtn.id = atn.node_id
   INNER JOIN account_account aa
    ON atn.account_id = aa.id
   INNER JOIN res_company com
    ON com.id = aa.company_id
   INNER JOIN res_currency curr
    ON curr.id = com.currency_id
   INNER JOIN coa_tree_calculated calculated_tree
    ON atn.account_id = calculated_tree.acc_id
  WHERE
   rtn.tree_id = $P!{REPORTING_TREE_ID}
  GROUP BY
   rtn.id,
   calculated_tree.comparison_id,
   calculated_tree.com_id
 ),
 reporting_tree_axes_data_points AS (
  SELECT
   rta.rtn_id,
   rta.rtn_parent_id,
   rta.rtn_level,
   rta.rtn_parent_left,
   rta.rtn_parent_right,
   rta.comparison_id,
   rta.com_order,
   rta.com_id,
   rtdp.bb_dr,
   rtdp.bb_cr,
   rtdp.dr,
   rtdp.cr,
   rtdp.move_lines_count,
   rtdp.move_lines,
   rtdp.bb_dr_cmp_1,
   rtdp.bb_cr_cmp_1,
   rtdp.dr_cmp_1,
   rtdp.cr_cmp_1,
   rtdp.move_lines_count_cmp_1,
   rtdp.move_lines_cmp_1
  FROM reporting_tree_axes rta
   LEFT JOIN reporting_tree_data_points rtdp
    ON (rta.rtn_id,
        rta.comparison_id,
        rta.com_id) = (rtdp.rtn_id,
                       rtdp.comparison_id,
                       rtdp.com_id)
 ),
 reporting_tree_calculated(rtn_id, rtn_parent_id, rtn_level, rtn_parent_left, rtn_parent_right,
                           comparison_id,
                           com_order, com_id,
                           bb_dr, bb_cr, dr, cr, move_lines_count, move_lines,
                           bb_dr_cmp_1, bb_cr_cmp_1, dr_cmp_1, cr_cmp_1, move_lines_count_cmp_1, move_lines_cmp_1) AS (
  (SELECT
    rtadp.rtn_id,
    rtadp.rtn_parent_id,
    rtadp.rtn_level,
    rtadp.rtn_parent_left,
    rtadp.rtn_parent_right,
    rtadp.comparison_id,
    rtadp.com_order,
    rtadp.com_id,
    rtadp.bb_dr,
    rtadp.bb_cr,
    rtadp.dr,
    rtadp.cr,
    rtadp.move_lines_count,
    rtadp.move_lines,
    rtadp.bb_dr_cmp_1,
    rtadp.bb_cr_cmp_1,
    rtadp.dr_cmp_1,
    rtadp.cr_cmp_1,
    rtadp.move_lines_count_cmp_1,
    rtadp.move_lines_cmp_1
   FROM reporting_tree_axes_data_points rtadp
   WHERE rtadp.rtn_level = (SELECT MAX(rtn_level)
                            FROM reporting_tree_axes)
  ) UNION ALL (
   WITH
    frozen_reporting_tree_calculated AS (
     SELECT * FROM reporting_tree_calculated
    )
   SELECT
    rtadp.rtn_id,
    rtadp.rtn_parent_id,
    rtadp.rtn_level,
    rtadp.rtn_parent_left,
    rtadp.rtn_parent_right,
    rtadp.comparison_id,
    rtadp.com_order,
    rtadp.com_id,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.bb_dr
                         END) + COALESCE(rtadp.bb_dr, 0.0),
                     rtadp.bb_dr)
      ELSE rtadp.bb_dr
     END) AS bb_dr,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.bb_cr
                         END) + COALESCE(rtadp.bb_cr, 0.0),
                     rtadp.bb_cr)
      ELSE rtadp.bb_cr
     END) AS bb_cr,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.dr
                         END) + COALESCE(rtadp.dr, 0.0),
                     rtadp.dr)
      ELSE rtadp.dr
     END) AS dr,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.cr
                         END) + COALESCE(rtadp.cr, 0.0),
                     rtadp.cr)
      ELSE rtadp.cr
     END) AS cr,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.move_lines_count
                         END) + COALESCE(rtadp.move_lines_count, 0.0),
                     rtadp.move_lines_count)
      ELSE rtadp.move_lines_count
     END) AS move_lines_count,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(GL_ARRAY_REDUCE(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.move_lines
                         END) || COALESCE(rtadp.move_lines, ARRAY[]::BIGINT[]),
                     rtadp.move_lines)
      ELSE rtadp.move_lines
     END) AS move_lines,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.bb_dr_cmp_1
                         END) + COALESCE(rtadp.bb_dr_cmp_1, 0.0),
                     rtadp.bb_dr_cmp_1)
      ELSE rtadp.bb_dr_cmp_1
     END) AS bb_dr_cmp_1,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.bb_cr_cmp_1
                         END) + COALESCE(rtadp.bb_cr_cmp_1, 0.0),
                     rtadp.bb_cr_cmp_1)
      ELSE rtadp.bb_cr_cmp_1
     END) AS bb_cr_cmp_1,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.dr_cmp_1
                         END) + COALESCE(rtadp.dr_cmp_1, 0.0),
                     rtadp.dr_cmp_1)
      ELSE rtadp.dr_cmp_1
     END) AS dr_cmp_1,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.cr_cmp_1
                         END) + COALESCE(rtadp.cr_cmp_1, 0.0),
                     rtadp.cr_cmp_1)
      ELSE rtadp.cr_cmp_1
     END) AS cr_cmp_1,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(SUM(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.move_lines_count_cmp_1
                         END) + COALESCE(rtadp.move_lines_count_cmp_1, 0.0),
                     rtadp.move_lines_count_cmp_1)
      ELSE rtadp.move_lines_count_cmp_1
     END) AS move_lines_count_cmp_1,
    (CASE
      WHEN (NOT rtn.dummy_node) AND rtn.calculation = 'sum'
       THEN COALESCE(GL_ARRAY_REDUCE(CASE
                          WHEN frtc_rtn.dummy_node = TRUE THEN NULL
                          ELSE frtc.move_lines_cmp_1
                         END) || COALESCE(rtadp.move_lines_cmp_1, ARRAY[]::BIGINT[]),
                     rtadp.move_lines_cmp_1)
      ELSE rtadp.move_lines_cmp_1
     END) AS move_lines_cmp_1
   FROM reporting_tree_axes_data_points rtadp
    INNER JOIN via_reporting_tree_node rtn
     ON rtadp.rtn_id = rtn.id
    LEFT JOIN frozen_reporting_tree_calculated frtc
     ON (rtadp.rtn_level = frtc.rtn_level - 1
         AND rtadp.rtn_parent_left < frtc.rtn_parent_left
         AND rtadp.rtn_parent_right > frtc.rtn_parent_right
         AND rtadp.comparison_id = frtc.comparison_id
         AND rtadp.com_id = frtc.com_id)
    LEFT JOIN via_reporting_tree_node frtc_rtn
     ON frtc.rtn_id = frtc_rtn.id
   WHERE
    rtadp.rtn_level = (SELECT DISTINCT rtn_level - 1
                       FROM frozen_reporting_tree_calculated)
   GROUP BY
    rtadp.rtn_id,
    rtadp.rtn_parent_id,
    rtadp.rtn_level,
    rtadp.rtn_parent_left,
    rtadp.rtn_parent_right,
    rtadp.comparison_id,
    rtadp.com_order,
    rtadp.com_id,
    rtn.dummy_node,
    rtn.calculation,
    rtadp.bb_dr,
    rtadp.bb_cr,
    rtadp.dr,
    rtadp.cr,
    rtadp.move_lines_count,
    rtadp.move_lines,
    rtadp.bb_dr_cmp_1,
    rtadp.bb_cr_cmp_1,
    rtadp.dr_cmp_1,
    rtadp.cr_cmp_1,
    rtadp.move_lines_count_cmp_1,
    rtadp.move_lines_cmp_1
  )
 ),
 reporting_tree_dummy_nodes_calculated AS (
  SELECT
   rtc.rtn_id,
   rtc.rtn_parent_id,
   rtc.rtn_level,
   rtc.rtn_parent_left,
   rtc.rtn_parent_right,
   rtc.comparison_id,
   rtc.com_order,
   rtc.com_id,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.bb_dr) + COALESCE(rtc.bb_dr, 0.0), rtc.bb_dr)
     ELSE rtc.bb_dr
    END) AS bb_dr,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.bb_cr) + COALESCE(rtc.bb_cr, 0.0), rtc.bb_cr)
     ELSE rtc.bb_cr
    END) AS bb_cr,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.dr) + COALESCE(rtc.dr, 0.0), rtc.dr)
     ELSE rtc.dr
    END) AS dr,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.cr) + COALESCE(rtc.cr, 0.0), rtc.cr)
     ELSE rtc.cr
    END) AS cr,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(rtc.move_lines_count, 0.0)
     ELSE rtc.move_lines_count
    END) AS move_lines_count,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN ARRAY[]::BIGINT[]
     ELSE rtc.move_lines
    END) AS move_lines,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.bb_dr_cmp_1) + COALESCE(rtc.bb_dr_cmp_1, 0.0), rtc.bb_dr_cmp_1)
     ELSE rtc.bb_dr_cmp_1
    END) AS bb_dr_cmp_1,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.bb_cr_cmp_1) + COALESCE(rtc.bb_cr_cmp_1, 0.0), rtc.bb_cr_cmp_1)
     ELSE rtc.bb_cr_cmp_1
    END) AS bb_cr_cmp_1,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.dr_cmp_1) + COALESCE(rtc.dr_cmp_1, 0.0), rtc.dr_cmp_1)
     ELSE rtc.dr_cmp_1
    END) AS dr_cmp_1,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(SUM(associates.cr_cmp_1) + COALESCE(rtc.cr_cmp_1, 0.0), rtc.cr_cmp_1)
     ELSE rtc.cr_cmp_1
    END) AS cr_cmp_1,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN COALESCE(rtc.move_lines_count_cmp_1, 0.0)
     ELSE rtc.move_lines_count_cmp_1
    END) AS move_lines_count_cmp_1,
   (CASE
     WHEN rtn.dummy_node AND rtn.calculation = 'sum'
      THEN ARRAY[]::BIGINT[]
     ELSE rtc.move_lines_cmp_1
    END) AS move_lines_cmp_1
  FROM reporting_tree_calculated rtc
   INNER JOIN via_reporting_tree_node rtn
    ON rtc.rtn_id = rtn.id
   LEFT JOIN via_reporting_tree_node_rel rtn_rel
    ON rtc.rtn_id = rtn_rel.node_id
   LEFT JOIN reporting_tree_calculated associates
    ON (rtn_rel.associate_id,
        rtc.comparison_id,
        rtc.com_id) = (associates.rtn_id,
                       associates.comparison_id,
                       associates.com_id)
  GROUP BY
   rtc.rtn_id,
   rtc.rtn_parent_id,
   rtc.rtn_level,
   rtc.rtn_parent_left,
   rtc.rtn_parent_right,
   rtc.comparison_id,
   rtc.com_order,
   rtc.com_id,
   rtn.dummy_node,
   rtn.calculation,
   rtc.bb_dr,
   rtc.bb_cr,
   rtc.dr,
   rtc.cr,
   rtc.move_lines_count,
   rtc.move_lines,
   rtc.bb_dr_cmp_1,
   rtc.bb_cr_cmp_1,
   rtc.dr_cmp_1,
   rtc.cr_cmp_1,
   rtc.move_lines_count_cmp_1,
   rtc.move_lines_cmp_1
 ),
 gl_result_set AS (
  SELECT (
   rtn.bold,
   rtn.underline,
   rtn.italic,
   rtn.move_bold,
   rtn.move_underline,
   rtn.move_italic,
   rtn.blank_line,
   rtn.title_line,
   rtn.sequence,
   rtdnc.comparison_id,
   (CASE rtdnc.comparison_id
     WHEN 1 THEN '$P!{REFERENCE_LABEL}'
     WHEN 2 THEN '$P!{COMPARISON_LABEL}'
     ELSE NULL
    END),
   rtdnc.com_order,
   rtdnc.com_id,
   com.name,
   (CASE
     WHEN $P!{USE_INDENTATION}
      THEN
       LPAD(rtn.name,
            (4 * (rtn.level
                  - FIRST_VALUE(rtn.level) OVER (ORDER BY rtn.level ASC))
             + LENGTH(rtn.name))::INTEGER)
     ELSE rtn.name
    END),
   rtn.level,
   (CASE rtdnc.comparison_id
     WHEN 1 THEN rtdnc.bb_dr - rtdnc.bb_cr
     WHEN 2 THEN rtdnc.bb_dr_cmp_1 - rtdnc.bb_cr_cmp_1
     ELSE NULL
    END),
   (CASE rtdnc.comparison_id
     WHEN 1 THEN rtdnc.dr
     WHEN 2 THEN rtdnc.dr_cmp_1
     ELSE NULL
    END),
   (CASE rtdnc.comparison_id
     WHEN 1 THEN rtdnc.cr
     WHEN 2 THEN rtdnc.cr_cmp_1
     ELSE NULL
    END),
   (CASE rtdnc.comparison_id
     WHEN 1 THEN rtdnc.dr - rtdnc.cr
     WHEN 2 THEN rtdnc.dr_cmp_1 - rtdnc.cr_cmp_1
     ELSE NULL
    END),
   (CASE rtdnc.comparison_id
     WHEN 1 THEN rtdnc.bb_dr - rtdnc.bb_cr + rtdnc.dr - rtdnc.cr
     WHEN 2 THEN rtdnc.bb_dr_cmp_1 - rtdnc.bb_cr_cmp_1 + rtdnc.dr_cmp_1 - rtdnc.cr_cmp_1
     ELSE NULL
    END),
   (CASE rtdnc.comparison_id
     WHEN 1 THEN rtdnc.move_lines_count
     WHEN 2 THEN rtdnc.move_lines_count_cmp_1
     ELSE NULL
    END),
   (CASE rtdnc.comparison_id
     WHEN 1 THEN rtdnc.move_lines
     WHEN 2 THEN rtdnc.move_lines_cmp_1
     ELSE NULL
    END))::GL_AXES_DATA_POINT AS datum
  FROM reporting_tree_dummy_nodes_calculated rtdnc
   INNER JOIN via_reporting_tree_node rtn
    ON rtn.id = rtdnc.rtn_id
   INNER JOIN res_company com
    ON com.id = rtdnc.com_id
 )
SELECT *
FROM gl_decorator(
 (SELECT ARRAY_AGG(datum)
  FROM gl_result_set),
 (SELECT ARRAY_AGG((id,
                    date,
                    am_name,
                    partner_name,
                    name || '  ' || COALESCE(am_narration, ''),
                    debit,
                    credit,
                    top_level_debit,
                    top_level_credit,
                    comparison_order,
                    company_id)::GL_JOURNAL_ITEM)
  FROM ((SELECT am.name AS am_name,
                am.narration AS am_narration,
                rp.name AS partner_name,
                1 AS comparison_order,
                ji.*
         FROM journal_items ji
          INNER JOIN account_move am
           ON ji.move_id = am.id
          LEFT JOIN res_partner rp
           ON ji.partner_id = rp.id
        ) UNION ALL (
         SELECT am.name AS am_name,
                am.narration AS am_narration,
                rp.name AS partner_name,
                2 AS comparison_order,
                ji.*
         FROM journal_items_cmp_1 ji
          INNER JOIN account_move am
           ON ji.move_id = am.id
          LEFT JOIN res_partner rp
           ON ji.partner_id = rp.id
        )) t
  WHERE
   $P!{DISPLAY_MOVE}
 )
)]]>
	</queryString>

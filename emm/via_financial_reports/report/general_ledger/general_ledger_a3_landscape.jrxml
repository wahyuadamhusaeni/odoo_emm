    GROUP BY
     ji.account_id,
     ji.company_id,
     acc_info.rates_chain,
     acc_info.conversion_date
   ),
   beginning_balance AS (
    SELECT
     ji.account_id AS acc_id,
     ji.company_id AS com_id,
     SUM(ji.debit) AS debit,
     SUM(ji.credit) AS credit,
     SUM(ji.top_level_debit) AS top_level_debit,
